#: thunderbird/start/release.lang:110
msgid "https://support.mozilla.org/questions/new/thunderbird"
msgstr "https://support.mozilla.org/questions/new/thunderbird"

#: thunderbird/start/release.lang:114
msgid "Features"
msgstr "Taq b'anikil"

#: thunderbird/start/release.lang:118
msgid "Visit our blog"
msgstr "Tatz'eta' ri qichinya'l"

#: thunderbird/start/release.lang:122
msgid "Contribute"
msgstr "Kato'on"

#: thunderbird/start/release.lang:126
msgid "Tips"
msgstr "Taq pixa'"

#: thunderbird/start/release.lang:130
msgid "Add-ons"
msgstr "Taq tz'aqat"

#: thunderbird/start/release.lang:134
msgid "Get help"
msgstr "Tak'ulu' ato'ik"
