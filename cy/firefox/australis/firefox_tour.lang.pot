#: firefox/australis/firefox_tour.lang:112
msgid "Frequently asked questions"
msgstr "Cwestiynau aml"

#: firefox/australis/firefox_tour.lang:116
msgid "Enjoy seamless browsing between devices with Sync"
msgstr "Mwynhau pori rhwydd rhwng dyfeisiau gyda Sync"

#: firefox/australis/firefox_tour.lang:120
msgid "Move or remove any button to match"
msgstr "Symud neu dynnu unrhyw fotwm i gydweddu"

